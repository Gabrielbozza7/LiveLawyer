generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DIRECT_URL")
  schemas  = ["auth", "public"]
}

model User {
  id           String    @id @db.Uuid
  lastName     String
  firstName    String
  profPicUrl   String?
  phoneNum     String
  email        String    @unique
  userType     UserType  @default(Civilian)
  sessionToken String?
  dateJoined   DateTime  @default(now())
  emerContacts Contact[]

  @@schema("public")
}

model Contact {
  id        String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userId    String?   @db.Uuid
  lastName  String
  firstName String
  picUrl    String?
  phoneNum  String
  email     String?
  type      UserType?
  user      User?     @relation(fields: [userId], references: [id], onDelete: Cascade)

  @@schema("public")
}

